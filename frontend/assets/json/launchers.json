{
  "title": "Launcher",
  "description": "Test Schema for Launcher",
  "type": "object",
  "required": [
    "name",
    "uid",
    "interface",
    "port",
    "apps",
    "logs"
  ],
  "properties": {
    "name": {
      "type": "string",
      "title": "Name",
      "default": ""
    },
    "uid": {
      "type": "string",
      "title": "UID",
      "default": "",
      "readOnly": true
    },
    "interface": {
      "type": "string",
      "title": "Network Interface",
      "default": "0.0.0.0"
    },
    "port": {
      "type": "number",
      "title": "Port",
      "default": 555
    },
    "apps": {
      "type": "string",
      "title": "Apps folder",
      "default": "./apps"
    },
    "logs": {
      "type": "string",
      "title": "Logs folder",
      "default": "./logs"
    }
  }
}